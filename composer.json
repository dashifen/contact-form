{
  "name": "dashifen/contact-form",
  "description": "A WordPress plugin that produces a contact form that can conscientiously either email or simply store messages in the database (or both).",
  "minimum-stability": "stable",
  "type": "wordpress-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "David Dashifen Kees",
      "email": "dashifen@dashifen.com",
      "role": "developer"
    }
  ],
  "require": {
    "php": ">=8.2",
    "dashifen/validator": "^3",
    "dashifen/wp-handler": "^11",
    "timber/timber": "^2"
  },
  "autoload": {
    "psr-4": {
      "Dashifen\\WordPress\\Plugins\\ConscientiousContactForm\\": "src"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:dashifen/validator.git"
    }
  ]
}
